{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["react", "react-dom"],
    "moduleResolution": "bundler",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["src"]
}
